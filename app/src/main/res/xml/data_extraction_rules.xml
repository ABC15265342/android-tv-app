<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- 排除敏感数据 -->
        <exclude domain="file" path="cache/" />
        <exclude domain="file" path="temp/" />
    </cloud-backup>
    <device-transfer>
        <!-- 设备转移时包含用户设置 -->
        <include domain="file" path="preferences/" />
    </device-transfer>
</data-extraction-rules>
